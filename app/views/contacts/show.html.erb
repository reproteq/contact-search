<div align="center">
<div class="w-50 p-3" style="background-color: #eee;">   
<p id="notice"><%= notice %></p>

 <p><%= link_to("Editar contacto", edit_contact_path, data: {toggle: 'tooltip' ,placement: 'top' , title: 'Editar Contacto' ,turbolinks: false } , :class => "btn btn-primary" ) %></p>

<p>
  <strong>Nombre:</strong>
  <%= @contact.nombre %>
</p>

<p>
  <strong>Apellidos:</strong>
  <%= @contact.apellidos %>
</p>

<p>
  <strong>Calle:</strong>
  <%= @contact.calle %>
</p>

<p>
  <strong>Poblacion:</strong>
  <%= @contact.poblacion %>
</p>

<p>
  <strong>Provincia:</strong>
  <%= @contact.provincia %>
</p>

<p>
  <strong>Pais:</strong>
  <%= @contact.pais %>
</p>

<%= link_to 'Editar', edit_contact_path(@contact) %> |
<%= link_to 'Contactos', contacts_path %>
</div>
</div>